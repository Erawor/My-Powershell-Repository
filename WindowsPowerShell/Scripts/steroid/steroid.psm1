#OnLoad
if ([Array]($ta = [Type]::GetType(
  'System.Management.Automation.TypeAccelerators'
))::Get.Keys -notcontains 'accelerators') {
  $ta::Add('accelerators', $ta)
}

Get-ChildItem $PSScriptRoot | ? {$_.PSIsContainer} |
% {Get-ChildItem "$($_.FullName)\*.ps1"} | % {.$_.FullName}
#OnRemove
$MyInvocation.MyCommand.ScriptBlock.Module.OnRemove = {
  [void][accelerators]::Remove('accelerators')
}

